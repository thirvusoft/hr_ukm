 "css": "*{\n    line-height:14px !important;\n    /*padding-top:5px !important;*/\n}\n.text{\n    float:right;\n    width:80%;\n}\n.left{\n    float:left;\n   width:50%;\n}\n.right{\n    float:right;\n    width:50%;\n}\n.img{\n    width:20%;\n    float:left;\n}\n.table1{\n    border: 1px solid black;\n    border-collapse: collapse;\n}\ntable, th, td{\n  border-top: 1px solid black;\n   /*border-bottom: 1px solid black;*/\n    /*border-left: 1px solid black;*/\n  border-collapse: collapse;\n}\n.pad{\n    padding-top:1px;\n    padding-left:px;\n}\n.small_box{\n    border-top: 1px solid black;\n    border-left: 1px solid black;\n    border-bottom: 1px solid black;\n    border-collapse: collapse;\n    float:left;\n    width:20px;\n    height:20px;\n}\n.empty{\n    float:left;\n    width:10px;\n    height:20px;\n}\n.emp{\n    float:left;\n}\n.box{\n    width:100%;\n    overflow:hidden;\n    /*border: 1px solid black;*/\n    border-collapse: collapse;\n    border-right:1px solid black;\n}\n.box-left{\n     float:left;\n     width:55%;\n     /*border-right: 1px solid black;*/\n     border-collapse: collapse;\n}\n.box-right{\n    float:right;\n    width:45%;\n    \n    border-top:1px solid black;\n}",
