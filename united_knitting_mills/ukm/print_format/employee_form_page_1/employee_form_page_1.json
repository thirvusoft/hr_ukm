 "html": "{% if letter_head and not no_letterhead -%}\n    {% if print_settings.repeat_header_footer %}\n        <br> <div class=\"letter-head\">{{ letter_head }}</div>\n    {%- endif %}\n{%- endif %}\n<center><h2><b>EMPLOYEE DETAILS</b></h2></center>\n<center><h2><b>\u0ba4\u0bca\u0bb4\u0bbf\u0bb2\u0bbe\u0bb3\u0bb0\u0bcd \u0baa\u0bb1\u0bcd\u0bb1\u0bbf\u0baf \u0bb5\u0bbf\u0baa\u0bb0\u0bae\u0bcd</b></h2></center>\n\n<div class='top-table'>\n    <div class='emp_details' >\n        <table style='width:100%'>\n            <tr>\n                <td style='width:60%'><p>NAME</p><p>\u0baa\u0bc6\u0baf\u0bb0\u0bcd</p></td>\n                <td><p style='text-align:left;float:left;'>:</p><center style='padding-top:4px'>{{doc.employee_name or ''}}</center></td>\n            </tr>\n            <tr>\n                <td><p>EMPLOYEE NO</p><p>\u0ba4\u0bca\u0bb4\u0bbf\u0bb2\u0bbe\u0bb3\u0bb0\u0bcd \u0b8e\u0ba3\u0bcd</p></td>\n                <td><p style='text-align:left;float:left;'>:</p><center style='padding-top:4px'>{{doc.name or ''}}</center></td>\n            </tr>\n            <tr>\n                <td><p>DESIGNATION</p><p>\u0baa\u0ba4\u0bb5\u0bbf</p></td>\n                <td><p style='text-align:left;float:left;'>:</p><center style='padding-top:4px'>{{doc.designation or ''}}</center></td>\n            </tr>\n            <tr>\n                <td><p>DEPARTMENT</p><p>\u0baa\u0bbf\u0bb0\u0bbf\u0bb5\u0bc1</p></td>\n                \n                <td><p style='text-align:left;float:left;'>:</p><center style='padding-top:4px'>{{doc.department or ''}}</center></td>\n            </tr>\n            <tr>\n                <td><p>EDUCATIONAL QUALIFICATION</p><p>\u0b95\u0bb2\u0bcd\u0bb5\u0bbf \u0ba4\u0b95\u0bc1\u0ba4\u0bbf</p></td>\n                <td><p style='text-align:left;float:left;'>:</p><center style='padding-top:4px'>\n                    {% set qualification = []%}\n                    {% set years = [] %}\n                    {% for row in doc.education %}\n                    {%set d = years.append(row.year_of_passing) %}\n                    {% if(years | max == row.year_of_passing) %}\n                    {% set a =  qualification.append(row.level) %}\n                    {% endif %}\n                    {%set d = years.append(row.year_of_passing) %}\n                    {% endfor %}\n                    {% if(qualification|len) %}\n                    {{qualification[-1]}}\n                    {% endif %}</center></td>\n            </tr>\n            <tr>\n                <td><p>DATE OF BIRTH / AGE</p><p>\u0baa\u0bbf\u0bb1\u0ba8\u0bcd\u0ba4 \u0ba4\u0bc7\u0ba4\u0bbf / \u0bb5\u0baf\u0ba4\u0bc1</p></td>\n                <td><p style='text-align:left;float:left;'>:</p><center style='padding-top:4px'>{{frappe.utils.formatdate(doc.get_formatted('date_of_birth'), \"dd-mm-yyyy\") or \"\"}}</center></td>\n            </tr>\n            <tr>\n                <td><p>MARITAL STATUS</p><p>\u0ba4\u0bbf\u0bb0\u0bc1\u0bae\u0ba3 \u0ba8\u0bbf\u0bb2\u0bc8</p></td>\n                <td><p style='text-align:left;float:left;'>:</p><center style='padding-top:4px'>{{doc.marital_status or ''}}</center></td>\n            </tr>\n        </table>\n    </div>\n    <div class='photo' style='height:100px;width:80px;'>\n        {% set photo = frappe.db.get_value('File', {'attached_to_name':doc.name}, 'file_url') %}\n        {% if photo %}\n        <img src={{photo}} alt=\"\">\n        {% endif %}\n    </div>\n</div>\n<table style='width:100%'></table>\n<p>&nbsp;</p>\n<table class='address' style='width:100%'>\n    <tr class='address' style='width:50%;'>\n        <td class='address'>\n            <P>PRESENT ADDRESS</P>\n            <P>\u0ba4\u0bb1\u0bcd\u0baa\u0bc7\u0bbe\u0ba4\u0bc8\u0baf \u0bae\u0bc1\u0b95\u0bb5\u0bb0\u0bbf</P>\n             <p>&nbsp;</p>\n            <p>{{doc.current_address or \"\"}}</p>\n            \n        </td>\n        <td class='address' style='width:50%;'>\n            <P> PERMANENT ADDRESS</P>\n            <P>  \u0ba8\u0bbf\u0bb0\u0ba8\u0bcd\u0ba4\u0bb0 \u0bae\u0bc1\u0b95\u0bb5\u0bb0\u0bbf</P>\n             <p>&nbsp;</p>\n            <p>{{doc.permanent_address or \"\"}}</p>\n        </td>\n    </tr>\n</table>\n<p>&nbsp;</p>\n<p>INCASE OF EMERGENCY CONTACT</p>\n<p>\u0b85\u0bb5\u0b9a\u0bb0\u0b95\u0bbe\u0bb2 \u0ba4\u0bca\u0b9f\u0bbe\u0bcd\u0baa\u0bc1\u0b95\u0bcd\u0b95\u0bc1</p>\n<table class='emergency' style='width:100%'>\n    <tr class='emergency' style='width:30%;'>\n        <td class='emergency'>\n            <p>NAME (\u0baa\u0bc6\u0baf\u0bb0\u0bcd)</p>\n            <p>&nbsp;</p>\n            <p>{{doc.person_to_be_contacted or ''}}</p>\n        </td>\n        <td class='emergency' style='width:40%;'>\n            <p>TELEPHONE NO (\u0ba4\u0bc6\u0bbe\u0bb2\u0bc8\u0baa\u0bc7\u0b9a\u0bbf \u0b8e\u0ba3\u0bcd)</p>\n            <p>&nbsp;</p>\n            <p>{{doc.emergency_phone_number or ''}}</p>\n        </td>\n        <td class='emergency' style='width:30%;'>\n            <p>RELATIONSHIP (\u0b89\u0bb1\u0bb5\u0bc1 \u0bae\u0bc1\u0bb1\u0bc8)</p>\n            <p>&nbsp;</p>\n            <p>{{doc.relationship or ''}}</p>\n        </td>\n    </tr>\n</table>\n<p>&nbsp;</p>\n<center><p>SOCIAL BENEFITS</p></center>\n\n<table class='address' style='width:50%;margin-left: auto;margin-right: auto;'>\n    <tr class='address' style='width:50%;'>\n        <td class='address'>\n            <P>E.S.I</P>\n            <p>&nbsp;</p>\n        </td>\n        <td class='address' style='width:50%;'>\n            <P> P.F</P>\n            <p>&nbsp;</p>\n        </td>\n    </tr>\n</table>\n <p>&nbsp;</p>\n<table class='address' style='width:50%;margin-left: auto;margin-right: auto;'>\n    <tr class='address'>\n        <td  style='text-align:left;width:50%;border-right:none !important;' >\n            <P>WAGES:\n            &nbsp;\n            {% set ssa = frappe.db.get_value('Salary Structure Assignment', {'employee':doc.name, 'docstatus':1}, 'base') %}\n            {% if(ssa) %}\n            {{ssa}}\n            {% endif %}\n            </P>\n            <p>\u0b9a\u0bae\u0bcd\u0baa\u0bb3\u0bae\u0bcd</p>\n        </td>\n        <td style='width:50%;text-align:left'>\n            <P>PER DAY / MONTH</P>\n            <p>\u0ba8\u0bbe\u0bb3\u0bcd / \u0bae\u0bbe\u0ba4\u0bae\u0bcd</p>\n        </td>\n    </tr>\n</table>\n <p>&nbsp;</p> <p>&nbsp;</p> <p>&nbsp;</p> <p>&nbsp;</p><p>&nbsp;</p>\n<div class='signature'>\n    <div class='emp_signature' style='width:50% !important;float:left;'>\n        <p>EMPLOYEE SIGNATURE</p>\n        <p>\u0ba4\u0bca\u0bb4\u0bbf\u0bb2\u0bbe\u0bb3\u0bb0\u0bcd \u0b95\u0bc8\u0baf\u0bca\u0baa\u0bcd\u0baa\u0bae\u0bcd</p>\n    </div>\n    <div class='auth_signature' style='width:50% !important;float:right;text-align:center'>\n       <p> AUTHORISED SIGNATURE</p>\n       <p>\u0ba8\u0bbf\u0bb0\u0bcd\u0bb5\u0bbe\u0b95\u0bbf \u0b95\u0bc8\u0baf\u0bca\u0baa\u0bcd\u0baa\u0bae\u0bcd</p>\n    </div>\n</div>\n <p>&nbsp;</p>\n\n<div style='float: left;'>\n    <p>DATE : {{frappe.utils.formatdate(frappe.utils.today(), \"dd-mm-yyyy\") }}</p>\n    <p>\u0ba8\u0bbe\u0bb3\u0bcd</p>\n</div>",
