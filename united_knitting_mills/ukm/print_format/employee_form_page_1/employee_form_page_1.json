 "css": "*{\n    line-height:13px !important;\n}\n.emp_details{\n    width:60%;\n    float:left;\n}\n.photo{\n    width:40%;\n    float:right;\n}\n.address,.emergency{\n    text-align:center;\n    border: 1px solid black;\n    border-collapse: collapse;\n}\n.signature, .emp_signature, .auth_signature{\n    width:100%;\n}",
